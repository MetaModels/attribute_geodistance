{
  "name": "metamodels/attribute_geodistance",
  "description": "MetaModels attribute for geo distance values",
  "license": "LGPL-3.0-or-later",
  "type": "contao-bundle",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "numeric",
    "geo",
    "distance"
  ],
  "authors": [
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://now.metamodel.me/",
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_geodistance/issues",
    "wiki": "https://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_geodistance"
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "contao-community-alliance/dc-general": "^2.3",
    "contao/core-bundle": "^4.13.0 <5.0",
    "doctrine/dbal": "^3.3",
    "menatwork/contao-multicolumnwizard-bundle": "^3.4",
    "metamodels/core": "^2.3.0@dev",
    "metamodels/filter_perimetersearch": "^2.3@dev",
    "symfony/dependency-injection": "^5.4",
    "symfony/http-kernel": "^5.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "phpcq/runner-bootstrap": "^1.0@dev"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeGeoDistanceBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\AttributeGeoDistanceBundle\\Test\\": "tests/Test"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": false,
      "contao/manager-plugin": false,
      "php-http/discovery": true
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-feature/2.3.0": "2.3.x-dev",
      "dev-hotfix/phpcq-2": "2.3.x-dev"
    },
    "contao-manager-plugin": "MetaModels\\AttributeGeoDistanceBundle\\ContaoManager\\Plugin"
  }
}
