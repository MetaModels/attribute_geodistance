{
  "name": "metamodels/attribute_geodistance",
  "description": "MetaModels attribute for geo distance values",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "numeric",
    "geo",
    "distance"
  ],
  "type": "contao-bundle",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
 "support": {
   "email": "mail@metamodel.me",
   "issues": "https://github.com/MetaModels/attribute_geodistance/issues",
   "wiki": "http://de.contaowiki.org/MetaModels",
   "irc": "irc://irc.freenode.org/contao.mm",
   "source": "https://github.com/MetaModels/attribute_geodistance"
 },
 "require": {
   "php": "^5.6 || ^7.0",
   "contao/core-bundle": "^4.4",
   "contao-community-alliance/dc-general": "^2.1@dev",
   "metamodels/core": "^2.1@dev",
   "menatwork/contao-multicolumnwizard": "^3.3",
   "symfony/dependency-injection": "^3.3"
 },
 "require-dev": {
   "cyberspectrum/contao-toolbox": "^0.7",
   "phpcq/all-tasks": "^1.2",
   "contao/manager-plugin": "^2.1",
   "metamodels/filter_perimetersearch": "^2.1@dev"
 },
 "autoload": {
   "psr-4": {
     "MetaModels\\AttributeGeoDistanceBundle\\": "src"
   }
 },
 "autoload-dev": {
   "psr-4": {
     "MetaModels\\AttributeGeoDistanceBundle\\Test\\": "tests/Test"
   }
 },
 "extra": {
   "transifex": {
   "project": "metamodels",
   "prefix": "attribute_geodistance-",
   "languages_cto": "contao/languages",
   "languages_tx": ".tx"
  },
 "runonce": [
   "runonce/runonce.php"
 ],
 "branch-alias": {
   "dev-master": "2.0.x-dev",
   "dev-feature/contao4": "2.1.x-dev"
   }
 },
 "repositories": [
   {
     "type": "vcs",
     "url": "git@gitlab.com:metamodels/core.git"
   }
 ]
}
